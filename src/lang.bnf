type Node struct {
        place string
        code []string
}

var tmpIndex int = 0
var labelIndex int = 0

// NewTmp generates a unique temporary.
func NewTmp() string {
        t := fmt.Sprintf("t%d", tmpIndex)
        tmpIndex++
        return t
}

// NewLabel generates a unique label name.
func NewLabel() string {
        l := fmt.Sprintf("l%d", labelIndex)
        labelIndex++
        return l
}

